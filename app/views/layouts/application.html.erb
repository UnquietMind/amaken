<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
  <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAgYzCKSSOPexEfMtLRaRB3cbOyHhaMYgM&callback=initMap">
  </script>
    
</head>
<body>

<%= render "layouts/header" %>

<% flash.each do |type, message| %>
  <div class="alert alert-info alert-dismissable" role="alert">
    <button type="button" class="close" data-dismiss="alert" aria-lable="Close"><span aria-hidden="true">&times;</span></button>
    <%= message %>
  </div>
<% end %>

<% if current_page?(root_path) %>
  <div class="landing">
<% else %>
  <div class="container">
<% end %>
    <%= yield %>
  </div>

</body>
</html>
